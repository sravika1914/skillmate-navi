{
  "name": "career-planning-system",
  "version": "1.0.0",
  "description": "A comprehensive career planning system with skill tracking and roadmap visualization",
  "main": "index.html",
  "scripts": {
    "build": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build:prod": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "dev": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "start": "npx http-server . -p 3000 -o"
